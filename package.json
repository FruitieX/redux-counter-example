{
  "name": "frontend-kit",
  "version": "1.0.0",
  "description": "Get started with frontend development",
  "scripts": {
    "postinstall": "niv material-ui@0.18.1 -d material-ui-old",
    "start": "webpack-dev-server --open --config webpack/webpack.config.dev.js",
    "start-nobrowser": "webpack-dev-server --config webpack/webpack.config.dev.js",
    "build": "rm -rf dist && webpack --config webpack/webpack.config.prod.js && cp -a assets/200.html dist/",
    "deploy": "rm -rf dist && webpack --config webpack/webpack.config.prod.js  && cp -a assets/200.html dist/ && surge ./dist --domain https://frontend-kit.surge.sh",
    "prod": "serve ./dist -s",
    "lint": "eslint --ext .jsx --ext .js src",
    "lint:fix": "eslint --ext .jsx --ext .js --fix src",
    "ngrok": "ngrok http -region eu 8080",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FruitieX/frontend-kit.git"
  },
  "keywords": [
    "react",
    "redux",
    "async"
  ],
  "author": "FruitieX",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FruitieX/frontend-kit/issues"
  },
  "homepage": "https://github.com/FruitieX/frontend-kit#readme",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-styled-components": "1.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.22.0",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.24.1",
    "jest": "^20.0.4",
    "react-test-renderer": "^15.4.1",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5"
  },
  "dependencies": {
    "history": "^4.6.1",
    "isomorphic-fetch": "^2.2.1",
    "jwt-decode": "^2.1.0",
    "lodash": "^4.17.4",
    "material-ui": "1.0.0-alpha.15",
    "npm-install-version": "^6.0.1",
    "offline-plugin": "^4.5.5",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-intl": "^2.2.3",
    "react-intl-redux": "^0.5.0",
    "react-redux": "^5.0.2",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "5.0.0-alpha.6",
    "react-style-proptype": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.5.2",
    "redux-act": "^1.1.0",
    "redux-api": "^0.10.5",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.0.0"
  }
}
